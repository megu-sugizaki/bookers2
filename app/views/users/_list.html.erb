

<!--New bookの部分-->
 <div class="mt-3">
  <h2>New book</h2>
  <%= form_with model: book do |f| %>
   <h4>Title</h4>
    <%= f.text_field :title %>
   <h4>Opinion</h4>
    <%= f.text_area :body %>
   <div class="mt-3">
    <%= f.submit 'Create Book', class: 'btn btn-success' %>
   </div>
  <% end %>
 </div>